<!Doctype html>
 <html>
   <head>
     <title>Dom Manipulation</title>
     <body>
       <h1 id="heading">
         This is original heading..
       </h1>
       <button onClick="changeText()" >
         Change Text
       </button>
     </body>
   </head>
 </html>

 ---------------- JS -----------------

 
function changeText () {
document.getElementById('heading').innerHTML ='Tushar';
}

